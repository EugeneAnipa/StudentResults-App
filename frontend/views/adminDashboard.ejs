<%- include('header'); -%>
<div class="container-fluid noti">
  <span class="z-n1 position-relative notibell">
    <svg
      xmlns="http://www.w3.org/2000/svg"
      height="24px"
      viewBox="0 -960 960 960"
      width="24px"
      fill="#c294bb"
    >
      <path
        d="M160-200v-80h80v-280q0-83 50-147.5T420-792v-28q0-25 17.5-42.5T480-880q25 0 42.5 17.5T540-820v28q80 20 130 84.5T720-560v280h80v80H160ZM480-80q-33 0-56.5-23.5T400-160h160q0 33-23.5 56.5T480-80Z"
      />
    </svg>
  </span>
  <span
    class="z-0 position-relative notinum"
    hx-get="/admin/poll"
    hx-trigger="every 5s"
  ></span>
</div>
<main>
  <div class="container" hx-post="/admin/" id="this" hx-swap="outerHTML">
    <% locals.students %>
    <div class="container-fluid">
      <ul>
        <% for(var i = 0; i < students.length;i++) { %>

        <li>
          <label
            ><a
              hx-post="/admin/student/<%= locals.students[i].email %>"
              value="<%= locals.students[i].email %>"
              hx-trigger="click"
              hx-target="#this"
            >
              <%= locals.students[i].firstname + " " +
              locals.students[i].lastname%>
            </a>
          </label>
        </li>

        <%}%>
      </ul>
    </div>
  </div>
</main>

<%- include('footer'); -%>
